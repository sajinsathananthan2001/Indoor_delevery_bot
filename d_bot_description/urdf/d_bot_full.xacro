    <!-- include's -->
    <xacro:include filename="$(find d_bot_description)/urdf/caster_transmission.xacro" />
    <xacro:include filename="$(find d_bot_description)/urdf/caster_holder_transmission.xacro" />
    <xacro:include filename="$(find d_bot_description)/urdf/gazebo_physics.xacro" />
    <xacro:include filename="$(find d_bot_description)/urdf/gazebo_sensors.xacro" />


    <xacro:macro name="default_inertial" params="mass">
        <inertial>
            <mass value="${mass}" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </xacro:macro>


